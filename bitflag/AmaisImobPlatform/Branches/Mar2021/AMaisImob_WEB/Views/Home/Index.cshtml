    if (ViewData.ContainsKey("UserRealEstateAgencyId"))
    {
        userRealEstateAgencyId = (int)ViewData["UserRealEstateAgencyId"];
    }
    int? realEstateId = null;
    if (User.IsInRealEstate())
    {

