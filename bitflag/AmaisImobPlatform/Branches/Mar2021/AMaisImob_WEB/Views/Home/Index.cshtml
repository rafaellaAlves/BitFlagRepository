<script type="text/javascript">
    function GetChart(certificadosAderidos) {

        var certs = certificadosAderidos.map(function (e, i) { return e.totalAdherence; });
        var categories = certificadosAderidos.map(function (e, i) { return e._MesAtual; });

        function MountChart() {
            Highcharts.chart('chart', {
                chart: {
                    backgroundColor: null,
                    type: 'line',
                    height: '300px'
                },
                credits: {
                    enabled: false
                },
                title: {
                    text: 'Evolução Carteira 12 meses',
                    align: 'left'
                },
                xAxis: {
                    categories: categories,
                    //lineColor: 'transparent'
                },
                yAxis: {
                    title: {
                        text: ''
                    },
                    gridLineColor: 'transparent'
                },
                plotOptions: {
                    line: {
                        dataLabels: {
                            enabled: true
                        },
                    }
                },
                legend: {
                    layout: 'vertical',
                    align: 'center',
                    verticalAlign: 'top',
                    floating: true,
                    backgroundColor: '#FFFFFF'
                },
                series: [{
                    name: 'Cert. Ativo',
                    data: certs,
                    color: '#f39a2d'
                }]
            });
        }

        MountChart();
    }
</script>

