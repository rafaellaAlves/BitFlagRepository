@{
    var lawTypes = (ViewData["LawTypes"] as List<DTO.Law.LawTypeViewModel>);
}
<div class="form-group">
    <label class="control-label" for="LawTypeId">@translationFunctions.GetTranslation("Tipo do Requisito Legal", User.FindFirstValue("CultureInfo"))</label>
    <select multiple="multiple" class="form-control ReportParameter" style="width: 100%;" data-hidden-parameter="LawTypeId">
        @if (lawTypes != null)
        {
            @foreach (var item in lawTypes)
            {
                <option value="@item.LawTypeId">@translationFunctions.GetTranslation(item.Name, User.FindFirstValue("CultureInfo"))</option>
            }
        }
    </select>
</div>