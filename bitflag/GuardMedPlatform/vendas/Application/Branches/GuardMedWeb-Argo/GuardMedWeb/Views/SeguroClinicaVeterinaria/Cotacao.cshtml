        display: none;
    }
    #InfoCard, #_MVInfoCard, .hide-when-small {
        display: block;
    }

    @@media screen and (max-width: 575px) {
        .colToResize, .hide-when-small, #InfoCard, #_MVInfoCard {
            display: none;
        }

        .tableToResize, .show-when-small {
            display: block;
        }

        .card {
            margin-top: .5em;
        }

        .tableFont12 td small{
            font-size:12px;
        }
