            </text>
        }
        PlanoExtensaoCoberturaComeco();
    }

    function PlanoExtensaoCoberturaComeco() { 
        @if (Model.PlanoExtensaoCoberturaId.HasValue)
        {
            <text>
        ConsultarCusto();
            if (@Model.PlanoExtensaoCoberturaId == 1) {
                planoExtensaoCoberturaPreco = $("#_PrecoTotalBasico").val();
                MVSelect(1, 'Profissional Básico', precoTotalBasico);
            }
            else if (@Model.PlanoExtensaoCoberturaId == 2) {
                planoExtensaoCoberturaPreco = $("#_PrecoTotalProI").val();
                MVSelect(2, 'Profissional I', precoTotalProI);
            }
            else if (@Model.PlanoExtensaoCoberturaId == 3) {
                planoExtensaoCoberturaPreco = $("#_PrecoTotalProII").val();
                MVSelect(3, 'Profissional II', precoTotalProII);
            }
            else if (@Model.PlanoExtensaoCoberturaId == 4) {
                MVSelect(4, 'Profissional III', precoTotalProIII);
                planoExtensaoCoberturaPreco = $("#_PrecoTotalProIII").val();
            }
        </text>
        }else if (Model.PlanoClinicaVeterinariaId.HasValue)
        {
            <text>
            planoExtensaoCoberturaPreco = "";
            MVSelect(null, 'Não Selecionado', '0,00');
            </text>
        }
    }

    function HasFinished() {
        return IsSeguroClinicaSelected && IsSeguroTecnicoSelected && IsSeguroMVSelected ? true : false;
    }

</script>