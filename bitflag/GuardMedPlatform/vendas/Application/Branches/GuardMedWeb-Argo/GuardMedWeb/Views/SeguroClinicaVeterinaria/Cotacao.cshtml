<script type="text/javascript">

    var IsSeguroClinicaSelected = false;
    var IsSeguroTecnicoSelected = false;
    var IsSeguroMVSelected = false;

    var planoExtensaoCoberturaPreco = "";
    var planoRTPreco = "";

    function _SeguroClinicaVeterinariaCotacaoSalvar() {
        var d = $('#_SeguroClinicaVeterinariaCotacaoForm').serializeArray();
        $.post('@Url.Action("Cotacao", "SeguroClinicaVeterinaria")', d, function (data) {
            window.location.href = '@Url.Action("Formulario", "SeguroClinicaVeterinaria")?reference=@Model.Referencia';
		});
    }

    function CalcularPreco() {
        var d = {
            PlanoClinicaVeterinariaId: $('#PlanoClinicaVeterinariaId').val(),
            planoRTPreco: planoRTPreco == undefined ? '0' : planoRTPreco.replace('.', ','),
            planoExtensaoCoberturaPreco: planoExtensaoCoberturaPreco.replace('.',','),
            QtdFuncionarios: $('#QtdFuncionarios').val()
        }
        $.ajax({
			url: '@Url.Action("ConsultarPreco", "SeguroClinicaVeterinaria")',
			type: 'POST',
			data: d,
			dataType: 'Json',
            success: function (data) {
                $('#_SeguroClinicaVeterinariaCotacaoPreco').text(data.toFixed(2).replace('.', ','));
                $('#PrecoTotal').val(data.toFixed(2).replace('.', ','));
            }
        });
    }

    function _SeguroClinicaVeterinariaCotacaoValidacao() {
        $('.text-danger').remove();
        var _seguroClinicaVeterinariaCotacaoErro = false;


        if (IsNullOrWhiteSpace($('#PlanoClinicaVeterinariaId').val())) {
            $('#CardGeral').after('<div class="text-danger text-right">Escolha um plano para sua clínica!</div>');
            _seguroClinicaVeterinariaCotacaoErro = true;
        }
        if ($('#planoRTInfoSelecionado').css('display') == 'none') {
            $('#RTMessageError').after('<div class="text-danger text-right">Escolha uma opção para o responsável técnico!</div>');
            _seguroClinicaVeterinariaCotacaoErro = true;
        }

        if ($('#planoMVInfoSelecionado').css('display') == 'none') {
            $('#_ExtensaoCoberturaCardGeral').after('<div class="text-danger text-right">Escolha um plano para extensão de cobertura!</div>');
            _seguroClinicaVeterinariaCotacaoErro = true;
        }

        return (!_seguroClinicaVeterinariaCotacaoErro);
    }

    var qtdFunc20, qtdFunc30, qtdFunc40, qtdFunc50, precoBasico20, precoBasico30, precoBasico40, precoBasico50, precoProI20, precoProI30, precoProI40, precoProI50, precoProII20, precoProII30, precoProII40, precoProII50, precoProIII20, precoProIII30, precoProIII40, precoProIII50, precoTotalBasico, precoTotalProI, precoTotalProII, precoTotalProIII, totalFunc;

    function ConsultarCusto() {
        $.ajax({
            url: '@Url.Action("Consulta", "SeguroProfissional")',
			type: 'POST',
			data: null,
            dataType: 'json',
            success: function (data) {
                qtdFunc20 = IsNullOrWhiteSpace($('#QtdFuncionarios20').val()) ? 0 : parseInt($('#QtdFuncionarios20').val());
                qtdFunc30 = IsNullOrWhiteSpace($('#QtdFuncionarios30').val()) ? 0 : parseInt($('#QtdFuncionarios30').val());
                qtdFunc40 = IsNullOrWhiteSpace($('#QtdFuncionarios40').val()) ? 0 : parseInt($('#QtdFuncionarios40').val());
                qtdFunc50 = IsNullOrWhiteSpace($('#QtdFuncionarios50').val()) ? 0 : parseInt($('#QtdFuncionarios50').val());
                precoBasico20 = (data[0].custoAssociadoMensal * qtdFunc20).toFixed(2); precoBasico30 = (data[1].custoAssociadoMensal * qtdFunc30).toFixed(2);
                precoBasico40 = (data[2].custoAssociadoMensal * qtdFunc40).toFixed(2); precoBasico50 = (data[3].custoAssociadoMensal * qtdFunc50).toFixed(2);
                precoProI20 = (data[4].custoAssociadoMensal * qtdFunc20).toFixed(2); precoProI30 = (data[5].custoAssociadoMensal * qtdFunc30).toFixed(2);
                precoProI40 = (data[6].custoAssociadoMensal * qtdFunc40).toFixed(2); precoProI50 = (data[7].custoAssociadoMensal * qtdFunc50).toFixed(2);
                precoProII20 = (data[8].custoAssociadoMensal * qtdFunc20).toFixed(2); precoProII30 = (data[9].custoAssociadoMensal * qtdFunc30).toFixed(2);
                precoProII40 = (data[10].custoAssociadoMensal * qtdFunc40).toFixed(2); precoProII50 = (data[11].custoAssociadoMensal * qtdFunc50).toFixed(2);
                precoProIII20 = (data[12].custoAssociadoMensal * qtdFunc20).toFixed(2); precoProIII30 = (data[13].custoAssociadoMensal * qtdFunc30).toFixed(2);
                precoProIII40 = (data[14].custoAssociadoMensal * qtdFunc40).toFixed(2); precoProIII50 = (data[15].custoAssociadoMensal * qtdFunc50).toFixed(2);
                precoTotalBasico = (parseFloat(precoBasico20) + parseFloat(precoBasico30) + parseFloat(precoBasico40) + parseFloat(precoBasico50)).toFixed(2);
                precoTotalProI = (parseFloat(precoProI20) + parseFloat(precoProI30) + parseFloat(precoProI40) + parseFloat(precoProI50)).toFixed(2);
                precoTotalProII = (parseFloat(precoProII20) + parseFloat(precoProII30) + parseFloat(precoProII40) + parseFloat(precoProII50)).toFixed(2);
                precoTotalProIII = (parseFloat(precoProIII20) + parseFloat(precoProIII30) + parseFloat(precoProIII40) + parseFloat(precoProIII50)).toFixed(2);
                totalFunc = qtdFunc20 + qtdFunc30 + qtdFunc40 + qtdFunc50;

                $('#totalFunc').empty().append(totalFunc); $('#_totalFunc').val(totalFunc);
                $('.PrecoBasico20').empty().append(precoBasico20.replace('.', ',')); $('.PrecoBasico30').empty().append(precoBasico30.replace('.', ','));
                $('.PrecoBasico40').empty().append(precoBasico40.replace('.', ',')); $('.PrecoBasico50').empty().append(precoBasico50.replace('.', ','));
                $('.PrecoTotalBasico').empty().append(precoTotalBasico.replace('.', ',')); $('#_PrecoTotalBasico').val(precoTotalBasico);
                $('.PrecoProI20').empty().append(precoProI20.replace('.', ',')); $('.PrecoProI30').empty().append(precoProI30.replace('.', ','));
                $('.PrecoProI40').empty().append(precoProI40.replace('.', ',')); $('.PrecoProI50').empty().append(precoProI50.replace('.', ','));
                $('.PrecoTotalProI').empty().append(precoTotalProI.replace('.', ',')); $('#_PrecoTotalProI').val(precoTotalProI);
                $('.PrecoProII20').empty().append(precoProII20.replace('.', ',')); $('.PrecoProII30').empty().append(precoProII30.replace('.', ','));
                $('.PrecoProII40').empty().append(precoProII40.replace('.', ',')); $('.PrecoProII50').empty().append(precoProII50.replace('.', ','));
                $('.PrecoTotalProII').empty().append(precoTotalProII.replace('.', ',')); $('#_PrecoTotalProII').val(precoTotalProII);
                $('.PrecoProIII20').empty().append(precoProIII20.replace('.', ',')); $('.PrecoProIII30').empty().append(precoProIII30.replace('.', ','));
                $('.PrecoProIII40').empty().append(precoProIII40.replace('.', ',')); $('.PrecoProIII50').empty().append(precoProIII50.replace('.', ','));
                $('.PrecoTotalProIII').empty().append(precoTotalProIII.replace('.', ',')); $('#_PrecoTotalProIII').val(precoTotalProIII);

                if ($('#PlanoExtensaoCoberturaId').val() == 1) {
                    planoExtensaoCoberturaPreco = $("#_PrecoTotalBasico").val();
                    CalcularPreco();
                    MVSelect(1, 'Profissional Básico', precoTotalBasico);
                } else if ($('#PlanoExtensaoCoberturaId').val() == 2) {
                    planoExtensaoCoberturaPreco = $("#_PrecoTotalProI").val();
                    CalcularPreco();
                    MVSelect(2, 'Profissional I', precoTotalProI);
                } else if ($('#PlanoExtensaoCoberturaId').val() == 3) {
                    planoExtensaoCoberturaPreco = $("#_PrecoTotalProII").val();
                    CalcularPreco();
                    MVSelect(3, 'Profissional II', precoTotalProII);
                } else if ($('#PlanoExtensaoCoberturaId').val() == 4) {
                    planoExtensaoCoberturaPreco = $("#_PrecoTotalProIII").val();
                    CalcularPreco();
                    MVSelect(4, 'Profissional III', precoTotalProIII);
                }
            }
        });
    }

    var RTprecoProI, RTprecoProII, RTprecoProIII;

    function ConsultarCustoRT() {
        if (IsNullOrWhiteSpace($('#NascimentoTecnico').val())) {
            $('#RTprecoProI').text('0,00');
            $('#RTprecoProII').text('0,00');
            $('#RTprecoProIII').text('0,00');
            return;
        }
        var _idade = 0;
        if ($('#NascimentoTecnico').val() == 1) _idade = 25;
        if ($('#NascimentoTecnico').val() == 2) _idade = 35;
        if ($('#NascimentoTecnico').val() == 3) _idade = 45;
        if ($('#NascimentoTecnico').val() == 4) _idade = 55;
        d = { idade: _idade };
        $.ajax({
            url: '@Url.Action("ConsultaRT", "SeguroProfissional")',
            type: 'POST',
            data: d,
            dataType: 'json',
            success: function (data) {
                if (data == false && data.length != 0) {
                    return;
                }
                if (data.length == 0) {
                    alert('Nenhum plano para esta idade.');
                    $('#_RTAdicionalCardGroup').hide();
                    //return;
                }
                else if (!IsSeguroTecnicoSelected) {
                    $('#_RTAdicionalCardGroup').show();
                }
                //pega preço
                RTprecoProI = data[0].custo.toFixed(2);
                $('#RTprecoProI').text(RTprecoProI.replace('.', ','));
                RTprecoProII = data[1].custo.toFixed(2);
                $('#RTprecoProII').text(RTprecoProII.replace('.', ','));
                RTprecoProIII = data[2].custo.toFixed(2);
                $('#RTprecoProIII').text(RTprecoProIII.replace('.', ','));

                //verifica qual o plano utilizado
                if ($('#PlanoResponsavelTecnicoId').val() == 2) {
                    planoRTPreco = RTprecoProI;
                    RTAdicionalSelect(2, 'Profissional I', RTprecoProI.replace('.', ','));
                    CalcularPreco();
                } else if ($('#PlanoResponsavelTecnicoId').val() == 3) {
                    RTAdicionalSelect(3, 'Profissional II', RTprecoProII.replace('.', ','));
                    planoRTPreco = RTprecoProII;
                    CalcularPreco();
                } else if ($('#PlanoResponsavelTecnicoId').val() == 4) {
                    RTAdicionalSelect(4, 'Profissional III', RTprecoProIII.replace('.', ','));
                    planoRTPreco = RTprecoProIII;
                    CalcularPreco();
                }
            }
        });
        }

</script>

